#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x156f061b0 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x156f1c060_0 .var "clk", 0 0;
v0x156f1c0f0_0 .var "d", 0 0;
v0x156f1c1a0_0 .net "q", 0 0, v0x156f1bc10_0;  1 drivers
v0x156f1c270_0 .net "qb", 0 0, L_0x156f1c3f0;  1 drivers
v0x156f1c320_0 .var "reset", 0 0;
S_0x156f04580 .scope module, "DFF" "dff" 2 10, 3 1 0, S_0x156f061b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
    .port_info 4 /OUTPUT 1 "qb";
L_0x156f1c3f0 .functor NOT 1, v0x156f1bc10_0, C4<0>, C4<0>, C4<0>;
v0x156f063a0_0 .net "clk", 0 0, v0x156f1c060_0;  1 drivers
v0x156f1bb70_0 .net "d", 0 0, v0x156f1c0f0_0;  1 drivers
v0x156f1bc10_0 .var "q", 0 0;
v0x156f1bca0_0 .net "qb", 0 0, L_0x156f1c3f0;  alias, 1 drivers
v0x156f1bd40_0 .net "reset", 0 0, v0x156f1c320_0;  1 drivers
E_0x156f0a300 .event posedge, v0x156f1bd40_0, v0x156f063a0_0;
S_0x156f1bea0 .scope task, "display" "display" 2 33, 2 33 0, S_0x156f061b0;
 .timescale 0 0;
TD_test.display ;
    %delay 1, 0;
    %vpi_call 2 34 "$display", "d:%0h, q:%0h, qb:%0h", v0x156f1c0f0_0, v0x156f1c1a0_0, v0x156f1c270_0 {0 0 0};
    %end;
    .scope S_0x156f04580;
T_1 ;
    %wait E_0x156f0a300;
    %load/vec4 v0x156f1bd40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x156f1bc10_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x156f1bb70_0;
    %assign/vec4 v0x156f1bc10_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x156f061b0;
T_2 ;
    %vpi_call 2 14 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000001 {0 0 0};
    %vpi_call 2 17 "$display", "Reset flop." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x156f1c060_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x156f1c320_0, 0, 1;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x156f1c0f0_0, 0, 1;
    %fork TD_test.display, S_0x156f1bea0;
    %join;
    %vpi_call 2 23 "$display", "Release reset." {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x156f1c0f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x156f1c320_0, 0, 1;
    %fork TD_test.display, S_0x156f1bea0;
    %join;
    %vpi_call 2 28 "$display", "Toggle clk." {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x156f1c060_0, 0, 1;
    %fork TD_test.display, S_0x156f1bea0;
    %join;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
